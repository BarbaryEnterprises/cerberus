<p id="notice"><%= notice %></p>

<h1>Vue Files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vue_files.each do |vue_file| %>
      <tr>
        <td><%= link_to 'Show', vue_file %></td>
        <td><%= link_to 'Edit', edit_vue_file_path(vue_file) %></td>
        <td><%= link_to 'Destroy', vue_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vue File', new_vue_file_path %>
